{
    "name": "humbug/humbug",
    "description": "Bah...",
    "keywords": ["testing", "mutation", "mutation testing"],
    "homepage": "https://github.com/padraic/humbug",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Padraic Brady",
            "email": "padraic.brady@gmail.com",
            "homepage": "http://blog.astrumfutura.com"
        }
    ],
    "support": {
        "issues": "https://github.com/padraic/humbug/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/process": "~2.6",
        "sebastian/diff": "~1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "mockery/mockery": "~0.9"
    },
    "suggest": {
        "ext-runkit": "Install from http://www.github.com/padraic/runkit. Do not use PECL!"
    },
    "autoload": {
        "psr-0": { "Humbug": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "Humbug\\Test": "tests/" }
    },
    "bin": ["bin/humbug"],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
